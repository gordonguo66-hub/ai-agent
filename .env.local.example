# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Encryption Key (SECURITY CRITICAL)
# - Development: Auto-generated on first run
# - Production: MUST be manually set in Vercel environment variables
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
# WARNING: If you lose this key, users will need to re-enter their API credentials
CREDENTIALS_ENCRYPTION_KEY=auto_generated

# Email (Resend)
RESEND_API_KEY=your_resend_api_key
EMAIL_FROM=support@coreboundai.io
SUPPORT_EMAIL=support@coreboundai.io

# Internal API Security
INTERNAL_API_KEY=your_secure_random_key
CRON_SECRET=your_cron_secret

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000
